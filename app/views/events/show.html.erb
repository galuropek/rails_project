<h5>title: <%= @event.title %></h5>
<p>description: <%= @event.description %></p>
<p>start_date: <%= @event.start %></p>
<p>end_date: <%= @event.end %></p>
<p>created_by: <%= User.find(@event.user_id).email %></p>

<%= link_to "Edit", edit_crew_event_path(@event), :class => 'btn btn-warning' %>
<%= link_to "Remove", crew_event_path(@event), method: :delete, data: {
    confirm: "Are you sure that you want to remove this Event?"
}, :class => 'btn btn-warning' %>